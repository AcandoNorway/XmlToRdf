@prefix xmlToRdf: <http://acandonorway.github.com/XmlToRdf/ontology.ttl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .

_:b0    a                  <htt://example.org/pnavn> ;
        xmlToRdf:hasValue  "a name" .

_:b1    a                  <htt://example.org/pnavn> ;
        xmlToRdf:hasValue  "a name" .

_:b2    a                  <htt://example.org/pnavn> ;
        xmlToRdf:hasValue  "another name" .

_:b3    a                  <htt://example.org/pnavn> ;
        xmlToRdf:hasValue  "a name" .

[ a                  <htt://example.org/A> ;
  xmlToRdf:hasChild  [ a                        <htt://example.org/B> ;
                       <htt://example.org/num>  "1" ;
                       xmlToRdf:hasChild        [ a                         <htt://example.org/name> ;
                                                  xmlToRdf:hasChild         _:b1 ;
                                                  xmlToRdf:hasMixedContent  ( "text " _:b1 ) ;
                                                  xmlToRdf:hasValue         "text"
                                                ] ;
                       xmlToRdf:hasChild        [ a                         <htt://example.org/name> ;
                                                  xmlToRdf:hasChild         _:b2 , _:b3 ;
                                                  xmlToRdf:hasMixedContent  ( "text " _:b3 " more text " _:b2 ) ;
                                                  xmlToRdf:hasValue         "text  more text"
                                                ] ;
                       xmlToRdf:hasChild        [ a                         <htt://example.org/name> ;
                                                  xmlToRdf:hasChild         _:b0 ;
                                                  xmlToRdf:hasMixedContent  ( "text " _:b0 " more text" ) ;
                                                  xmlToRdf:hasValue         "text  more text"
                                                ]
                     ]
] .
